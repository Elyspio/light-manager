{
	"name": "ts-node-template",
	"version": "1.0.0",
	"description": "NodeJS with typescript support template",
	"author": "Jonathan Guichard",
	"license": "MIT",
	"private": false,
	"dependencies": {
		"@reduxjs/toolkit": "^1.3.5",
		"argparse": "^1.0.10",
		"async-mutex": "^0.2.2",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"express-winston": "^4.0.3",
		"fs-extra": "^9.0.0",
		"redux": "^4.0.5",
		"redux-logger": "^3.0.6",
		"socket.io": "^2.3.0",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@types/argparse": "^1.0.38",
		"@types/cors": "^2.8.6",
		"@types/express": "^4.17.6",
		"@types/fs-extra": "^8.1.0",
		"@types/node": "latest",
		"@types/socket.io": "^2.1.4",
		"nodemon": "latest",
		"pkg": "latest",
		"pm2": "^4.4.0",
		"source-map-support": "latest",
		"ts-node": "^8.10.1",
		"typescript": "latest"
	},
	"scripts": {
		"start-back": "nodemon  src/app.ts",
		"build-back": "nodemon -e ts,json --watch \"./src\" -x \"tsc\"",
